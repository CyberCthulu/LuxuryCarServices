{
  "name": "luxurycarservices",
  "version": "1.0.0",
  "scripts": {
    "install": "npm install --prefix backend",
    "start": "npm start --prefix backend",
    "migrate": "npx sequelize-cli db:migrate --env production",
    "seed": "npx sequelize-cli db:seed:all --env production",
    "build": "npm install && npm run migrate && npm run seed"
  },
  "engines": {
    "node": "18.x"
  }
}

